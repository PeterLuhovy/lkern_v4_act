{
  "name": "@l-kern/config",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/config/src",
  "projectType": "library",
  "tags": ["type:shared", "scope:config"],
  "targets": {
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit --project packages/config/tsconfig.lib.json"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/packages/config"],
      "options": {
        "passWithNoTests": false,
        "reportsDirectory": "../../coverage/packages/config"
      }
    }
  }
}